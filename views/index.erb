<table>
  <tr>
    <th>Title</th>
    <th>Job Link</th>
    <th>Description</th>
    <th>Company Name</th>
    <th>Company URL</th>
    <th>Location</th>
    <th>Post Date</th>
  </tr>
  <% jobs.each do |job| %>
  <tr>
    <td><%= job.title %></td>
    <td><a href="<%= job.link %>"</td>
    <td><%= job.desc %></td>
    <td><%= job.company %></td>
    <td><%= job.company_url %></td>
    <td><%= job.location %></td>
    <td><%= job.date %></td>
  </tr>

  <% end %>
